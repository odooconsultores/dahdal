<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

    <record id="view_country_state_dahdal_tree" model="ir.ui.view">
        <field name="name">res.country.state.dahdal.tree</field>
        <field name="model">res.country.state</field>
        <field name="inherit_id" ref="base.view_country_state_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="region"/>
            </xpath>
        </field>
    </record>

    <record id="view_country_state_dahdal_form" model="ir.ui.view">
        <field name="name">res.country.state.dahdal.form</field>
        <field name="model">res.country.state</field>
        <field name="inherit_id" ref="base.view_country_state_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="region"/>
            </xpath>
        </field>
    </record>

    </data>
</odoo>